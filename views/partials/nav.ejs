<nav class="navbar">
    <div class="navbar-menu navbar-end">
      <% if (user) { %>
          <p class="navbar-item"><%= user.user %></p>
      <% } %>
      <!-- routes -->
      <% if (routes) { %> <% routes.forEach((route) => { %>
          <a class="navbar-item" href="<%= route.href %>"> <%= route.title %> </a>
      <% }) %> <% } %>
  </div>
</nav>